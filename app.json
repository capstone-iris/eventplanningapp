{
	"expo": {
		"name": "eventplanningapp",
		"slug": "eventplanningapp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.reactnativefirebase",
			"config": {
				"googleSignIn": {
					"reservedClientId": "com.googleusercontent.apps.325804923316-icsvkf4fc3mh166i4t33tkjdepltr67p"
				}
			},
			"googleServicesFile": "./GoogleService-Info.plist"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
